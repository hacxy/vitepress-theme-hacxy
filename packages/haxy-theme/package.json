{
  "name": "vitepress-theme-haxy",
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "nodemon",
    "build": "rimraf ./dist && unbuild -c ./build.config.ts"
  },
  "keywords": [
    "vitepress",
    "vitepress-theme",
    "vitepress-theme-haxy"
  ],
  "license": "MIT",
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "exec": "echo",
    "events": {
      "start": "npm run build",
      "restart": "npm run build"
    },
    "ext": ".vue,.ts,.json"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "nodemon": "^3.1.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitepress": "^1.0.1"
  },
  "dependencies": {
    "oh-my-live2d": "^0.15.2",
    "vue": "^3.4.21"
  }
}
